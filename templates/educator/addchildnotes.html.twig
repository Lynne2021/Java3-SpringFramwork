{% extends "master.html.twig" %}

{% block content %}
{% if errorList %}
    <ul>
    {% for error in errorList %}
        <li>{{ error }}</li>
    {% endfor %}
    </ul>
{% endif %}
<h3>Group Child Notes:</h3>

   <form method="post"enctype="multipart/form-data" >
       <input type="text" hidden name="id"  value="{{child.id}}"><br/>
    First Name: <input type="text" name="firstName" value="{{child.firstName}}"><br/>
    Last Name: <input type="text" name="lastName" value="{{child.lastName}}"><br/>
    Image: <input type="file" name="image"><br>
    Height(cm): <input type="textr" name="height" value="{{v.height }}"><br>
    Weight(kg): <input type="text" name="weight" value="{{ v.weight }}"><br>
    Skill: <input type="text" name="skills" value="{{ v.skills}}"><br>
    Notes: <textarea name="note">{{ v.note}}</textarea><br>
   
    <input type="submit" value="submit">
</form> 
{% endblock content %}
 